generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Produto {
  id            String   @id @default(auto()) @map("_id") @db.ObjectId
  nome          String
  descricao     String
  fornecedor    String
  categoria     Int
  subcategoria  Int
  preco         Float
  promocao      Boolean
  bucket        String
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}
